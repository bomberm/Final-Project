<h1> Edit Contents </h1>
<form>
	<fieldset>
		<legend>Edit Contents</legend>
		<input type="number" id="myID" style="visibility:collapse;" value={{id}}>
		<label>Name</label>
		<input type="text" name="name" id="name" value={{name}}>
		<label> Reps </label>
		<input type="number" name="reps" id="reps" value={{reps}}>
		<label> Weight </label>
		<input type="number" name="weight" id="weight" value={{weight}}>
		<label> Date </label>
		<input type="date" name="date" id="date" value={{date}}>
		<label> Weight Type </label>
		<select name="type" id="type" value={{lbs}}>
	   <option value="1">Pounds</option>
	   <option value="0">Kilograms</option>
		<select>
		<button onClick="finish();return false;" value={{id}}>Update</button>
	</fieldset>
</form>